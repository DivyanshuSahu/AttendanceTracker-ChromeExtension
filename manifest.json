{
  "manifest_version": 2,

  "name": "Time-Pass",
  "description": "Keep track of your attendence",
  "version": "2.0",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "Time-Pass"
  },
  "permissions": [
    "activeTab"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
